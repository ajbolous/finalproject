
echo "Setting up env"
export PYTHONPATH=$PYTHONPATH:$(pwd)/application:$(pwd)
echo "Stopping running servers"
pkill python
pkill gulp
pkill node
echo "Starting Django Server..."
python server/manage.py runserver 0.0.0.0:8000 &
python server/wsserver.py &
node server/server-stream.js 311988 8082 8084 base64&
cd client
echo "Starting HTTP server gulp"
gulp serve&
cd ..
